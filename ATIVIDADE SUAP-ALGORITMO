    export function problem (lines){
     const raio = Number(lines[0])
     const pi = 3.14159

     const area = pi * raio ** 2

     console.log(`A=${area}`)

    }

    //1035//
    export function problem (lines){
        const A = Number(lines[0])
        const B = Number(lines[1])
        const C = Number(lines[2])
        const D = Number(lines[3])

        if ( B > C && D > A && C + D > A + B && C > 0 && D > 0&& A % 2 ===0){
            console.log("Valores aceitos")
        }
        else{
            console.log("Vlaores nao aceitos")
        }

        }

        //1044//
        export function problem (lines){
            const A = Number(lines[0])
            const B = Number(lines [1])

            if ( A % B == 0 || B % A == 0 ){
                console.log("SAO MULTIPLOS!")
            }  else{
                console.log("NAO SAO MULTIPLOS!")
            }


        }
        //1046//
        export function problem (lines){
            const hora1 = Number(lines [0])
            const hora2 = Number(lines[1])

            const duraçao = hora2 - hora1

            console.log("O JOGO DUROU ", duraçao , " HORA(S).")


        }


        //1048//
        export function problem (lines){
            const  valor =Number(lines[0])
            let percentual;
            
            if( valor <= 400){
              percentual = 15;

        } else if (valor > 400 && valor <= 800){
            percentual = 12;
        } else if (valor > 800  && valor <= 1200){
            percentual = 10;
        } else if ( valor > 1200 && valor <=2000){
            percentual = 7;
        }
        else {
            percentual = 4;
        }
          const aumento = valor + (valor * percentual/100);
          const novoSalario = valor + aumento;

          console.log(`aumento`, ${aumento} );
          console.log(`percentual:", ${ percentual} % `);
          console.log("novo salario:", ${novoSalario});
    

    }
    //1038//
var input = require("fs").readFileSync("/dev/stdin", "utf8");
var lines = input.split(" ");

let codigo = parseInt(lines[0]); 
let quantidade = parseInt(lines[1]);

let preco;

if (codigo === 1) {
    preco = 4.00;
} else if (codigo === 2) {
    preco = 4.50;
} else if (codigo === 3) {
    preco = 5.00;
} else if (codigo === 4) {
    preco = 2.00;
} else if (codigo === 5) {
    preco = 1.50;
}

let total = preco * quantidade;

console.log("Total: R$ " + total.toFixed(2));

