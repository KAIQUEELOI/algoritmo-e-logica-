//1045//

et [l1,l2,l3] = lines[0].split(' ').map(Number)

let aux 
if  (l1 < l2){
    aux = l1 
    l1 = l2
    l2 = aux
}
if(l1 < l3){
    aux = l1
    l1 = l3
    l3 = aux
}
if (l2 < l3){
    aux = l2 
    l2 = l3
    l3 = aux
}


if (l1 >= l2 + l3){
    console.log("NAO FORMA TRIANGULO")

}
else if ( l1^2 == l2^2 + l3^2){
    console.log("TRIANGULO RETANGULO")
}
else if( l1^2 > l2^2 + l3^2){
    console.log("TRIANGULO OBTUSANGULO")
}
else if( l1^2 < l2^2 + l3^2 ){
    console.log("TRIANGULO ACUTANGULO")
}else if(l1 == l2 == l3){
    console.log("TRIANGULO EQUILATERO")
} 
else if (l1 == l2 || l1 == l3 || l2 == l3){
    console.log("TRIANGULO ISOCELES")
}

//1047//
const [h1,m1,h2,m2] = lines[0].split(' ').map(Number)

const inicio = h1 * 60 + m1;
const fim = h2 * 60 + m2;

let duracaoTotalMinutos 

if (fim > inicio){
    //seja no mesmo dia//
    duracaoTotalMinutos = fim - inicio
}else{

    duracaoTotalMinutos = (24 * 60 - inicio) + fim
}

const duracaoHoras = Math.floor(duracaoTotalMinutos/60)
const duracaoMinutos = duracaoTotalMinutos % 60

console.log(`O JOGO DUROU ${duracaoHoras} HORAS (S) E ${duracaoMinutos} MINUTOS (S)`)

//1101//
lines.forEach( line => {
    const [m, n] = line.split(' ')
                     .map(Number)
                     .sort( ( a,b ) => a - b )
   if (m <= 0 || n <= 0) return
   let sequencia = []
   let soma = 0
   for( let numero = m; numero <= n ; numero++){
    sequencia.push(numero)
    soma += numero
   }
   console.log(`${sequencia.join(' ')} sum=${soma}`)
})

